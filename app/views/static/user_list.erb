<div class="container">
<div class="table-responsive">          
	  <table class="table" id="data_table">
	    <thead>
	      <tr>
	      	<th>ID</th>
	        <th>Name</th>
	   		<th>Email</th>
	      </tr>
	    </thead>
	    <tbody>
	       	<% @all_users.each do |x| %>
	  		<tr>
	  			<td><%= x.id %></td>
	 	   		<td><%= x.name %></td>
	    		<td><%= x.email %></td>
	  		</tr>
	  		<%end%>
	    </tbody>
	  </table>
</div>
</div>